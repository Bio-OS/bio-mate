        {
            "type": "number",
            "key": "SigLabelSize",
            "label": "相关系数字体大小",
            "placeholder": "请输入相关系数字体大小值"
        },
        {
            "type": "select",
            "options": [
                { "label": "original", "value": "original" },
                { "label": "AOE", "value": "AOE" },
                { "label": "FPC", "value": "FPC" },
                { "label": "hclust", "value": "hclust" },
                { "label": "alphabet", "value": "alphabet" }
            ],
            "key": "ArrangeBy",
            "label": "坐标排序根据",
            "required": true,
            "placeholder": "请输入",
            "help": "请从original, AOE,FPC, hclust, alphabet选择一个"
        }
