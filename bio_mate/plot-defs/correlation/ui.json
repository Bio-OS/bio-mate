        {
            "type": "number.4",
            "key": "mars",
            "label": "图四边空出距离",
            "help": "四个值分别代表（bottom, lef, top, right）",
            "placeholder": "请输入图例字体大小值"
        },
        {
            "type": "number",
            "key": "siglevel",
            "label": "显著水平阈值",
            "placeholder": "请输入显著水平阈值"
        },
        {
            "type": "number",
            "key": "pchsize",
            "label": "不显著的标签大小",
            "placeholder": "请输入不显著的标签大小值"
